const mongoose = require("mongoose");

const info_schema = mongoose.Schema({
  username: {
    type: String,
    required: true,
  },
  currency: {
    type: String,
    required: true,
  },
  initial_amount: {
    type: Number,
    required: true,
  },
  weekly_plan: {
    type: Number,
    required: true,
  },
});

module.exports = mongoose.model("info_db", info_schema);
